    "version": "0.2.0",
    "configurations": [
        {
            "type": "firefox",
            "request": "launch",
            "name": "Launch Firefox",
            "url": "http://localhost:53333/",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "presenterremote",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "presenterremote (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "presenterremote (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}